#!/bin/sh 
#PBS -l nodes=1:ppn=1
#PBS -N nonfeimi
#PBS -e job.err
#PBS -o job.out
#PBS -l walltime=240:00:00
#PBS -q queue_bigmem
out=date.out

cd $PBS_O_WORKDIR

EXE=./skyrmionlattice
NP=`cat $PBS_NODEFILE | wc -l`
echo Job starts at `date` >$out

mpirun -machinefile $PBS_NODEFILE -np $NP $EXE 

echo Job finishes at `date` >>$out
